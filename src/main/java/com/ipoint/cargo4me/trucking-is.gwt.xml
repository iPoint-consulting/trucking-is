<?xml version='1.0' encoding='UTF-8'?>
<module rename-to='cargo4me'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<!-- <inherits name='com.google.gwt.inject.Inject'/> -->

	<!-- Default css -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->
	<inherits name='com.gwtplatform.dispatch.Dispatch' />

	<inherits name='com.gwtplatform.mvp.Mvp' />
	
	<inherits name='com.google.web.bindery.requestfactory.RequestFactory' />

	<inherits name="com.github.gwtbootstrap.Bootstrap" />

	<inherits name="com.google.web.bindery.autobean.AutoBean" />
	<inherits name="com.google.gwt.json.JSON" />

	<entry-point class="com.ipoint.cargo4me.client.IntegrationServiceEntryPoint" />

	<source path='client' />
	<source path='shared' />

	<set-configuration-property name="gin.ginjector"
		value="com.ipoint.cargo4me.client.IntegrationServiceGinjector" />


	<!-- Default client module -->
	<!-- <set-configuration-property name="gin.ginjector.modules" value="com.ipoint.cargo4me.client.gin.ClientModule"/> -->

	<!-- Bootstrapper -->
	<!-- <set-configuration-property name="gwtp.bootstrapper" value="com.ipoint.cargo4me.client.gin.BootstrapperImpl"/> -->

</module>
