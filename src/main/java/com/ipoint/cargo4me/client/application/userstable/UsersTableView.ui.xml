<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	xmlns:c="urn:import:com.google.gwt.user.cellview.client" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	
	<ui:style>
		.top_row {
			padding: 0 15px;
		}
		.table {
			padding: 0 10px;
		}
	</ui:style>	

	<g:HTMLPanel>		
			<b:FluidRow styleName="{style.top_row}">
				<b:Column size="9">
					<b:CheckBox visible="false" ui:field="checkBoxSelectAll">Отменить выбор</b:CheckBox>
				</b:Column>
				<b:Column size="3">
					<b:SimplePager ui:field="pager" />
				</b:Column>
			</b:FluidRow>
		<g:HTMLPanel styleName="{style.table}" ui:field="tableContainer"/>
	</g:HTMLPanel>
</ui:UiBinder>



